{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "tailorshop\\.uz"
        }
      ],
      "destination": "https://www.tailorshop.uz/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "/api/sitemap" },
    { "source": "/image-sitemap.xml", "destination": "/api/image-sitemap" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
